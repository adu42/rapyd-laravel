{
    "name": "adu42/rapyd",
    "description": "crud widgets for laravel, to make an admin in few lines of code",
    "homepage": "https://github.com/adu42/rapyd-laravel",
    "keywords": ["laravel", "crud", "rapyd", "administrator", "admin", "backend", "datatable", "form"],
    "authors": [
        {
            "name": "zofe",
            "email": "felice.ostuni@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "cyberwolf888/html": "^7.*",
        "intervention/image": "*",
        "adu42/burp": "master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:adu42/burp.git"
        }
    ],
    "suggest": {
        "baum/baum": "Allows to use the DataTree widget"
    },
    "autoload": {
        "classmap": [
            "migrations"
        ],
        "psr-4": {
            "Zofe\\Rapyd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zofe\\Rapyd\\RapydServiceProvider"
            ]
        }
    },
    "require-dev": {
        "illuminate/support": "*",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
